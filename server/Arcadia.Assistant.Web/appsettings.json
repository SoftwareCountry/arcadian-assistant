{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "TimeoutSeconds": 10,
  "SslOffloading": false,
  "HelpLink": "https://arcadiaspb.sharepoint.com/teams/ArcadiaAssistant/SitePages/Usage%20Guide.aspx",
  "Server": {
    "ActorSystemName": "arcadia-assistant",
    //"Host": "arcadia.assistant.server.console",
    "Host": "0.0.0.0",
    "Port": 63301
  },
  "Security": {
    "ClientId": "<insert here>",
    "AuthorizationUrl": "<insert here>",
    "TokenUrl": "<insert here>",
    "OpenIdConfigurationUrl": "https://login.microsoftonline.com/{tenantId}/.well-known/openid-configuration",
    "SwaggerRedirectUri": "http://localhost:63601/swagger/o2c.html"
  },
  "ServiceEndpointsAuthentication": {
    "Realm": "Arcadia Assistant",
    "Login": "service_user",
    "Password": "service_user" 
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "DownloadApplication": {
    "DownloadBuildIntervalMinutes": 10,
    "BuildsFolder": "Builds",
    "RenameBuildFilePattern": "app_release_{buildNumber}{ext}",
    "ApiToken": "",
    "AndroidGetBuildsUrl": "",
    "AndroidGetBuildDownloadLinkTemplateUrl": "",
    "IosGetBuildsUrl": "",
    "IosGetBuildDownloadLinkTemplateUrl": ""
  },
  "UpdateNotification": {
    "NotificationTitle": "Update available",
    "NotificationBody": "New application version is available."
  } 
}