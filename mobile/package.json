{
  "name": "ArcadiaAssistant",
  "version": "0.0.1",
  "private": true,
  "watch": {
    "lint": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "ts",
        "tsx"
      ]
    }
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "copyImages": "copyfiles -u 1 src/**/*.png src/**/*.jpg artifacts",
    "build": "npm run clean && npm run lint && npm run copyImages && npm run tsc --",
    "watch": "concurrently \"npm-watch lint\" \"npm run build -- -w\"",
    "prepare": "npm run build",
    "lint": "tslint -p ."
  },
  "dependencies": {
    "@haskkor/react-native-pincode": "^1.20.0",
    "appcenter": "^1.10.0",
    "appcenter-analytics": "^1.10.0",
    "appcenter-crashes": "^1.10.0",
    "appcenter-push": "^1.10.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.22.2",
    "react": "16.6.3",
    "react-native": "^0.58.6",
    "react-native-config": "^0.11.6",
    "react-native-fast-image": "^5.2.0",
    "react-native-fingerprint-scanner": "^2.5.0",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-modal": "^7.0.1",
    "react-native-sensitive-info": "^5.2.6",
    "react-native-vector-icons": "^6.1.0",
    "react-navigation": "^3.0.2",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "santee-dcts": "^0.9.5",
    "url-search-params-polyfill": "^5.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/react": "^16.7.7",
    "@types/react-native": "^0.57.14",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-redux": "^6.0.10",
    "@types/react-test-renderer": "^16.6.3",
    "@types/redux-logger": "^3.0.6",
    "babel-jest": "23.6.0",
    "react-dom": "^16.8.3",
    "react-art": "^16.3.2",
    "react-native-web": "^0.9.13",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.49.2",
    "npm-watch": "^0.5.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "cacheDirectory": ".jest/cache"
}
