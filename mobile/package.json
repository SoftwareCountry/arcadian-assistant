{
  "name": "ArcadiaAssistant",
  "version": "0.0.1",
  "private": true,
  "watch": {
    "lint": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "ts",
        "tsx"
      ]
    }
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "copyImages": "copyfiles -u 1 src/**/*.png src/**/*.jpg artifacts",
    "build": "npm run clean && npm run lint && npm run copyImages && npm run tsc --",
    "watch": "concurrently \"npm-watch lint\" \"npm run build -- -w\"",
    "prepare": "npm run build",
    "lint": "tslint -p ."
  },
  "dependencies": {
    "@haskkor/react-native-pincode": "^1.21.1",
    "appcenter": "^2.6.1",
    "appcenter-analytics": "^2.6.1",
    "appcenter-crashes": "^2.6.1",
    "appcenter-push": "^2.6.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "buffer": "^5.4.3",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "moment-business": "^3.0.1",
    "react": "^16.9.0",
    "react-native": "^0.61.5",
    "react-native-config": "^0.12.0",
    "react-native-fast-image": "^7.0.2",
    "react-native-fingerprint-scanner": "^3.0.2",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-keychain": "^4.0.5",
    "react-native-modal": "^7.0.1",
    "react-native-sensitive-info": "^5.5.5",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.0.2",
    "react-redux": "^7.1.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.5.4",
    "santee-dcts": "^0.9.5",
    "uuid": "^3.3.2",
    "whatwg-url": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@types/jest": "^24.9.0",
    "@types/moment-business": "^0.1.4",
    "@types/react": "^16.9.0",
    "@types/react-native": "^0.60.30",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.0",
    "@types/redux-logger": "^3.0.6",
    "@types/whatwg-url": "^6.4.0",
    "babel-jest": "24.7.1",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.56.0",
    "npm-watch": "^0.5.0",
    "react-art": "^16.3.2",
    "react-dom": "^16.8.3",
    "react-native-typescript-transformer": "^1.2.10",
    "react-native-web": "^0.9.13",
    "react-test-renderer": "16.9.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "cacheDirectory": ".jest/cache"
}
